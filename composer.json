{
    "name": "chuyenlv/drupal8",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.0.20",
        "drupal-composer/drupal-scaffold": "^2.0.1",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "~8.2",
        "drupal/block_field": "^1.0",
        "drupal/components": "^1.0",
        "drupal/paragraphs": "^1.0",
        "drupal/metatag": "1.x-dev",
        "drupal/redirect": "1.x-dev",
        "drupal/pathauto": "1.0-beta1",
        "drupal/xmlsitemap": "1.x-dev",
        "drupal/adminimal_theme": "^1.2",
        "drupal/file_browser": "^1.0@alpha",
        "drupal/file_entity": "^2.0@beta",
        "drupal/media_entity": "^1.5",
        "drupal/inline_entity_form": "1.0-beta1",
        "drupal/image_widget_crop": "^1.4",
        "drupal/media_entity_image": "^1.2",
        "drupal/viewsreference": "1.0.0-alpha8",
        "drupal/entity_browser": "^1.0@beta",
        "drupal/entity_embed": "^1.0@beta",
        "drupal/media_entity_browser": "1.x-dev",
        "drupal/address": "1.0.0-rc3",
        "drupal/admin_toolbar": "1.17",
        "drupal/color_field": "2.0-rc1",
        "drupal/libraries": "3.x-dev",
        "drupal/video_embed_field": "1.3",
        "drupal/adminimal_admin_toolbar": "^1.3",
        "drupal/google_analytics": "^2.1",
        "drupal/twig_field_value": "^1.1",
        "drupal/menu_breadcrumb": "^1.0",
        "drupal/workbench_access": "1.0-alpha2",
        "drupal/default_content": "^1.0@alpha",
        "drupal/search_api": "^1.0",
        "drupal/webform": "^5.0@beta",
        "drupal/search_api_solr": "^1.0@beta",
        "drupal/focal_point": "1.0-beta4",
        "drupal/twig_tweak": "^1.4",
        "drupal/media_entity_document": "^1.1",
        "drupal/views_vcards": "2.x-dev",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/acquia_purge": "^1.0@alpha",
        "drupal/acquia_connector": "^1.7",
        "drupal/acquia_search": "^1.7",
        "drupal/migrate_plus": "~3.0",
        "drupal/migrate_tools": "~3.0",
        "drupal/migrate_upgrade": "~3.0",
        "drupal/entity_reference_revisions": "1.x-dev",
        "drupal/chosen": "^2.1",
        "drupal/blazemeter": "^1.0",
        "drupal/devel": "^1.0@RC",
        "drupal/shield": "^1.0",
        "drupal/memcache": "^2.0@alpha",
        "drupal/allowed_formats": "^1.0",
        "drupal/link_attributes": "~1.0",
        "drupal/view_unpublished": "^1.0@alpha",
        "drupal/asset_injector": "^1.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drush/drush": "^8.1",
        "drupal/console": "~1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@drupal-scaffold"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                "sites/default/default.settings.php",
                "sites/default/default.services.yml"
            ],
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        },
        "patches": {
            "drupal/allowed_formats": {
              "Allow text format prompts to be turned off": "https://www.drupal.org/files/issues/allow_to_control_if-2617982-24.patch"
            },
            "drupal/core": {
                "Add verbose error messages to config dependency errors": "https://www.drupal.org/files/issues/2655104-56.patch",
                "Add support for Views to datetime_range": "https://www.drupal.org/files/issues/improve_the_views_integration_for_daterange_fields-2786577-62.patch",
                "Exposed date filter leads to a notice": "https://www.drupal.org/files/issues/2811887-19-fix-tests.patch",
                "Use form element of type date instead textfield when selecting a date in an exposed filter": "https://www.drupal.org/files/issues/2648950-71.patch"
            },
            "drupal/search_api": {
                "Fix issue with date filters not being allowed on views": "https://www.drupal.org/files/issues/2843854-22-search_api-date_filter_schema.patch"
            }
        }
    }
}
